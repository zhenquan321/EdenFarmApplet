.nav_fixed {
    position: fixed;
    background-color: #fff;
    height: 40px;
    width: 100%;
    font-size: 15px;
    text-align: center;
    z-index: 9999;
    top: 0;
    max-width: 640px;
}
.nav_txt {
    position: relative;
}

.nav_cur {
    border-bottom: 2px solid #1aad19;
    color: #1aad19;
    font-weight:bold;
}

.fixed_nav_item {
    height: 40px;
    line-height: 40px;
    width: 25%;
    float: left;
    display: block;
    box-sizing: border-box;
    border-bottom: 1px solid #ccc;
}

#dealliststatus1 {
    font-size: 12px;
    padding-top: 42px;
}

.order {
    background-color: #fff;
    border-top: 1px solid #e4e4e4;
    border-bottom: 1px solid #dedede;
}

.express_loading {
    display: block;
    position: absolute;
    left: 50%;
    top: 40%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    width: 32px;
    height: 32px;   
}

.img_rotate {
    width: 30px;
    height: 30px;
    margin-top: 7px;
    margin-right: 5px;
    -webkit-animation: loading 1s linear 0s infinite;
}

.center {
    width: 170px;
    margin: 0 auto;
    text-align: center;
}

.order {
    font-size: 24rpx;
    margin:20rpx;
    border-radius:20rpx; 
    padding-bottom:8rpx;

}

.order_hd {
    height: 39px;
    line-height: 39px;
    overflow: hidden;
    padding: 0 10px;
}

.order_bd {
    padding: 0 10px;
}

.order_bd {
    background-color: #fff;
    padding: 0 10px;
}

.order_goods {
    position: relative;
    padding: 15px 0;
    padding-left: 75px;
    font-size: 12px;
    border-bottom: 1px solid #d7d7d7;
    min-height: 60px;
}
.order_goods_img {
    position: absolute;
    top: 7px;
    left: 0;
}

.order_goods_img .img {
    display: block;
    width: 60px;
    height: 60px;
    border: 1px solid #d7d7d7;
}

.order_goods_info {
    position: relative;
}

.order_goods_name {
    max-height: 30px;
    color: #000;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-bottom: 5px;
    line-height: 15px;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

.order_goods_attr {
    color: #666;
}

.order_goods_attr_item {
    line-height: 22px;
    height: 24px;
}

.order_goods_price {
    float: right;
}

.order_goods_price {
    color: #1aad19;
    float: right;
    font-size: 16px;
    height: 22px;
    line-height: 22px;
}

.order_goods_attr_item {
    line-height: 22px;
    overflow: hidden;
    height: 24px;
}


.order_goods_attr_item:after {
    clear: both;
    content: "";
    display: block;
    height: 0;
    visibility: hidden;
}

.order_ft {
    padding: 10px;
}

.order_total {
    font-size: 12px;
    height: 16px;
    line-height: 16px;
    overflow: hidden;
    color: #5a5a5a;
        text-align: right;
            margin-bottom: 10px;
}

.order_total_info {
    float: left;
}

.order_price {
    float: right;
}

.order_price .b {
    color: #1aad19;
    font-size: 16px;
    font-weight: 400;
}

.small {
    font-size: 80%;
}

.order_total:after {
    content: '';
    display: block;
    clear: both;
    height: 0;
    visibility: hidden;
}

.order_opt {
    padding: 10px 0 0;
}

.order_status {
    float: left;
    height: 30px;
    line-height: 30px;
    color: #1aad19;
}

.order_btn {
    float: right;
}

.order_btn .a {
    float: left;
    width: 80px;
    height: 30px;
    background-color: #aaa;
    line-height: 30px;
    border-radius: 3px;
    color: #fff;
    margin-left: 5px;
    text-align: center;
}

.order_btn_buy {
    background-color: #1aad19!important;
}

.order_btn_receive {
    background-color: #1aad19!important;
}

.order_opt:after {
    content: '';
    display: block;
    clear: both;
    height: 0;
    visibility: hidden;
}

.nav_cur {
    border-bottom: 2px solid #1aad19;
    color: #1aad19;
}

.p {
    display: inline;
}

.body {
    margin:0 auto;
    min-width:320px;
    max-width:640px;
    height:100%;
    background:#fff;
    font-size:12px;
    font-family:Helvetica,STHeiti STXihei,Microsoft JhengHei,Microsoft YaHei,Arial;
    line-height:1.5;
    color:#666;
    -webkit-text-size-adjust:100%!important;
    -webkit-user-select:none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
    background-color:#f1f1f2;
}

page {
    background-color:#eceded;
}
.li {display: inline;}

.dialog_content {
    background-color: #fff;
    border-radius: 6px;
    z-index: 9999;
    position: fixed;
    width: 90%;
    height: 77%;
    left: 50%;
    top: 50%;
    max-width: 620px;
    -webkit-transform: translate(-50%,-47%);
    transform: translate(-50%,-47%);
}
.express_header {
    color: #111;
    font-weight: 700;
    display: inline-block;
    font-size: 30rpx;
    margin: 10px 0 0 20px;
}

.right_pos {
    float: right;
    margin-right: -13px;
    margin-top: -13px;
    width: 30px;
    height: 30px;
    display: block;
    text-align: center;
    border-radius: 100%;
}

.cancel_icon {
    width: 48px;
    height:48px;
    display:inline;
    text-align: center;
    color: #fff;
}

.express_main {
    margin: 20px auto 0;
    width: 95%;
    height: 76%;
    overflow: scroll;
}

.express_html{
  background-color:#fafafa;
  width:100%;
  height:100%;
  border: 1px dashed #ccc;
  border-radius: 10rpx;
}
